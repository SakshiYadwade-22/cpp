#include <iostream>
using namespace std;

int main() {
    int n;
    // cout << "Enter the size of the array: " << endl;
    cin >> n;

    int arr[n];
    // cout << "Enter the elements of the array: " << endl;

    // Taking input from the user
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    // Printing the elements in reverse order
    for (int i = n - 1; i >= 0; i--) {
        cout << arr[i] << " ";
    }
    cout << endl;
    return 0;
}
