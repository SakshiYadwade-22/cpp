#include <iostream>
#include <sstream>
using namespace std;
class Student{
    public:
    int age;
    string first_name;
    string last_name;
    int standard;
};

int main() {
    Student st;
    cin>>st.age>>st.first_name>>st.last_name>>st.standard;
    cout<<st.age<<"\n"<<st.last_name<<","<<" "<<st.first_name<<"\n"<<st.standard<<"\n"<<endl;
    cout<<st.age<<","<<st.first_name<<","<<st.last_name<<","<<st.standard<<endl;
    
    return 0;
}
